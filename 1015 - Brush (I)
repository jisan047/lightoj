#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,cs=0;
    cin>>t;
    while(t--)
    {
        int n,ans=0;
        cin>>n;
        for(int i=0;i<n;i++)
        {
            int x;
            cin>>x;
            if(x>0)
                ans+=x;
        }
        printf("Case %d: %d\n",++cs,ans);
    }
    return 0;
}
