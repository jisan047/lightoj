#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int t,cs=0;
    cin>>t;
    while(t--)
    {
        ll a[3];
        for(int i=0;i<3;i++)cin>>a[i];
        sort(a,a+3);
        if((a[0]*a[0]+a[1]*a[1])==a[2]*a[2])
            printf("Case %d: yes\n",++cs);
        else
            printf("Case %d: no\n",++cs);
    }
    return 0;
}
