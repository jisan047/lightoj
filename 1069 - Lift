#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,cs=0;
    cin>>t;
    while(t--)
    {
        int a,b;
        cin>>a>>b;
        if(b<a)
        {
            printf("Case %d: %d\n",++cs,4*(a+(a-b))+19);
        }
        else
            printf("Case %d: %d\n",++cs,4*b+19);
    }
}
