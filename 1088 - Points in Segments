int arr[N];
int n;

int lowerbound(int x){
    int low=0;
    int high=n-1;
    int mid;

    while(low<=high){
        mid=(low+high)>>1;
        if(arr[mid]<x)low=mid+1;
        else high=mid-1;
    }
    return low;
}

int upperbound(int y){
    int low=0;
    int high=n-1;
    int mid;

    while(low<=high){
        mid=(low+high)>>1;
        if(arr[mid]>y)high=mid-1;
        else low=mid+1;
    }
    return low;
}

// loading..................


int main()
{
    int t=Int,cs=0;
    while(t--){
        n=Int;
        int q=Int;
        loop(i,n)arr[i]=Int;
        pf("Case %d:\n",++cs);
        loop(i,q){
            int x=Int,y=Int;
            //cout<<upperbound(y)<<" "<<lowerbound(x)<<endl;
            pf("%d\n",(upperbound(y)-lowerbound(x)));
        }
    }

    End;
}
